package se.publicclass.debug {	import flash.utils.getTimer;							/**	 * @author Robert Sk√∂ld, robert(at)publicclass.se	 */	public class Log {		private var _level : int;		private var _time : int;		private var _messages : Array;		private var _stackTrace : StackTrace;		public function Log( level : int , messages : Array , stackTrace : StackTrace = null ) {			_time = getTimer( );			_level = level;			_messages = messages;			_stackTrace = stackTrace || new StackTrace();		}		public function get time() : int {			return _time;		}		public function get stackTrace() : StackTrace {			return _stackTrace;		}		public function get level() : int {			return _level;		}		public function set level( level : int ) : void {			_level = level;		}		public function get messages() : Array {			return _messages;		}		public function set messages( messages : Array ) : void {			_messages = messages;		}	}}
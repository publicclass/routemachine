package se.publicclass.example.command {	import se.publicclass.example.ApplicationRoutes;	import org.puremvc.as3.interfaces.INotification;	import org.puremvc.as3.patterns.command.SimpleCommand;	import org.puremvc.as3.utilities.routes.RouteMachine;	import org.puremvc.as3.utilities.routes.SWFAddressMediator;	/**	 * @author Robert Sk√∂ld, robert(at)publicclass.se	 */	public class SetupRoutesCommand extends SimpleCommand {				override public function execute(notification : INotification) : void {			facade.registerMediator( new RouteMachine( ApplicationRoutes ) );			facade.registerMediator( new SWFAddressMediator( ApplicationRoutes.START ) );						// If we skip the SWFAddressMediator we'd like to do this to get started://			sendNotification( RouteMachine.GOTO , {} , ApplicationRoutes.START );		}	}}
package se.publicclass.utils {	/**	 * @author Robert Sk√∂ld, robert(at)publicclass.se	 */	public class UrlUtil {				/**		 * Since a crossdomain.xml needs to be in the root, and Amazon S3 also has a subdomains		 * to access their bucket we rewrite the url to go through the subdomain instead of the		 * directory. 		 */		public static function fixS3( url : String ) : String {			if( !url ) return null;			return url.replace( /http:\/\/(s3.amazonaws.com)\/([.a-z0-9_-]+)\/(.+)/ig , "http://$2.$1/$3" );		}				public static function cleanSWFAddress( url : String ) : String {			return url.replace( "#/" , "" );		}			}}